@model IEnumerable<Cargo>

@{
    ViewBag.Title = "Ваші вантажі";
}


@if (Model == null)
{
    <h3>У вас ще не мае вантажів</h3>
    <a asp-controller="Cargo" asp-action="CreateCargo">Cтворіть новий</a>
}
else
{
    <ul>
        @foreach (var c in Model)
        {
            <li>
                <span>Один з вантажів - @c.CargoName</span>
            </li>
        }
    </ul>
}
